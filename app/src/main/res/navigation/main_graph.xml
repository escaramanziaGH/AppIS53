<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.posta10.Fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registrarseFragment"
            app:destination="@id/registrarseFragment" />
        <action
            android:id="@+id/action_loginFragment_to_datosPersonalesFragment22"
            app:destination="@id/datosPersonalesFragment2" />
    </fragment>
    <fragment
        android:id="@+id/registrarseFragment"
        android:name="com.example.posta10.Fragments.RegistrarseFragment"
        android:label="fragment_registrarse"
        tools:layout="@layout/fragment_registrarse" />
    <fragment
        android:id="@+id/datosPersonalesFragment2"
        android:name="com.example.posta10.Fragments.DatosPersonalesFragment"
        android:label="DatosPersonalesFragment" />
</navigation>